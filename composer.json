{
    "name": "xitox97/artisan-clear",
    "description": "Package that extends Laravel's artisan command",
    "keywords": [
        "laravel",
        "artisan-clear"
    ],
    "homepage": "https://github.com/spatie/artisan-clear",
    "license": "MIT",
    "authors": [
        {
            "name": "Farhan Hadi",
            "email": "farhan.abdhadi@gmail.com",
            "homepage": "https://farhanhadi.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Xitox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xitox\\ArtisanClearServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
